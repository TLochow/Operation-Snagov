}

[node name="Label3" type="Label" parent="Labels"]
modulate = Color( 1, 1, 1, 0 )
anchor_right = 1.0
anchor_bottom = 1.0
custom_fonts/font = SubResource( 2 )
text = "Nicolae liked to rule, electing family members as officials and striking down everyone who opposed him, as his nation watched along, not being able to do anything about it."
align = 1
valign = 1
autowrap = true
__meta__ = {
"_edit_use_anchors_": false
}

[node name="Label4" type="Label" parent="Labels"]
